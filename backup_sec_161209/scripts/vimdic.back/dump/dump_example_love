<script type="text/javascript">
var _tiq = 'undefined' !== typeof _tiq ? _tiq : []; // Global Variables
_tiq.push(['__trackEvent', 'qlog', 'love', 'no_result', '']);

var exposeIds = null;
if (false) {
    var contentNoArray = new Array();
    jQuery.each($('.\\#word'), function() {
    	var titles = $('strong', $(this));
    	for (var index = 0; index < titles.length; index++) {
    		if(titles[index].className.indexOf('@') >= 0) {
    			var contentNo = titles[index].className.substring(titles[index].className.indexOf('@') + 1);
    			if (contentNo.indexOf(' ') >= 0) {
    				contentNo = contentNo.substring(0, contentNo.indexOf(' '));
    			}
    			contentNoArray.push(contentNo);
    		}
    	}
    });
    exposeIds = contentNoArray.join(',');
} else {
    var contentSectionArray = new Array();
    jQuery.each($('.\\#word'), function() {
    	var section = $(this).data('target') || '';
    	var contentNoArray = new Array();
    	var titles = $('strong', $(this));
    	for(var index = 0; index < titles.length; index++) {
    		if(titles[index].className.indexOf('@') >= 0) {
    			var contentNo = titles[index].className.substring(titles[index].className.indexOf('@') + 1);
    			if (contentNo.indexOf(' ') >= 0) {
    				contentNo = contentNo.substring(0, contentNo.indexOf(' '));
    			}
    			contentNoArray.push(contentNo);
    		}
    	}
    	contentSectionArray.push(section + '={' + contentNoArray.join(',') +  '}');
    });
    exposeIds = contentSectionArray.join('|');
}

if (exposeIds) {
	_tiq.push(['__trackEvent', 'expose', exposeIds, '', '']);
}
</script>



